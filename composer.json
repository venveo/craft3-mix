{
    "name": "venveo/craft3-mix",
    "description": "Helper plugin for rev-manifest in Craft templates",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "mix"
    ],
    "support": {
        "docs": "https://github.com/venveo/craft3-mix/blob/master/README.md",
        "issues": "https://github.com/venveo/craft3-mix/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "email": "info@venveo.com"
        },
        {
            "name": "Ransom Roberson",
            "email": "ransom@venveo.com"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.20"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "venveo\\mix\\": "src/"
        }
    },
    "extra": {
        "name": "Mix",
        "handle": "craft3-mix",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/venveo/craft3-mix/master/CHANGELOG.md",
        "components": {
            "mixService": "venveo\\mix\\services\\MixService"
        },
        "class": "venveo\\mix\\Mix"
    }
}
